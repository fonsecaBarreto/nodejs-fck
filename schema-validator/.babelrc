        "@babel/preset-env",
        "@babel/preset-typescript"
    ],
    "plugins": [
        ["module-resolver", {
            "root": ["./dist"],
            "alias": {
                "@": "./src"
            }
        }]
    ],
    "ignore": [
        "**/*.spec.ts",
        "**/*.test.ts"
    ]
}